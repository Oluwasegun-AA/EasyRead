{
  "name": "MultiClip",
  "version": "0.0.1",
  "main": "dist/app/index.js",
  "scripts": {
    "build": "webpack",
    "clear": "rimraf dist build",
    "pretty": "./node_modules/.bin/prettier --single-quote --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "eslint src/**/*.js --fix",
    "test:pre-commit": "CI=true react-scripts test --colors -u --coverage",
    "contributors:init": "all-contributors init",
    "contributors:add": "npx all-contributors add",
    "contributors:generate": "npx all-contributors generate",
    "test": "react-scripts test --watchAll -u --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:pre-commit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.4.5",
    "babel-eslint": "10.0.3",
    "node-sass": "4.12.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "all-contributors-cli": "6.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.4",
    "image-webpack-loader": "5.0.0",
    "lint-staged": "9.2.3",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react-redux": "7.1.0",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0"
  }
}
